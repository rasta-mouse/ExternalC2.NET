<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>disable</Nullable>
        <LangVersion>10</LangVersion>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>0.1</PackageVersion>
        <Title>External C2</Title>
        <Authors>Daniel Duggan, Ryan Hanson</Authors>
        <Description>.NET Standard library which implements Cobalt Strike's External C2 specification (revision	0.1).</Description>
        <PackageProjectUrl>https://www.cobaltstrike.com/downloads/externalc2spec.pdf</PackageProjectUrl>
        <RepositoryUrl>https://github.com/rasta-mouse/ExternalC2</RepositoryUrl>
    </PropertyGroup>

</Project>
